<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    
    <!-- 选中状态 -->
    <item android:state_selected="true" android:color="@color/primary" />
    
    <!-- 按下状态 -->
    <item android:state_pressed="true" android:color="@color/primary_variant" />
    
    <!-- 获得焦点状态 -->
    <item android:state_focused="true" android:color="@color/primary" />
    
    <!-- 默认状态 -->
    <item android:color="@color/text_primary_dark" />
    
</selector>
